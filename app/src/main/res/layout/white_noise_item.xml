<?xml version="1.0" encoding="utf-8"?>
<com.example.simple.simplethink.totle.fragment.whiteNoisePage.MyLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android_custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="95dp"
    android:layout_height="95dp"
    android:background="@drawable/white_item"
    android:id="@+id/white_item"
    android:orientation="vertical">
    <ImageView
        android:id="@+id/white_selected"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/selected"
        android:scaleType="fitXY"
        android:layout_alignParentRight="true"
        android:layout_gravity="right"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="1"
        android:visibility="gone"
        />
    <ImageView
        android:id="@+id/recycle_white_sign"
        android:layout_width="10dp"
        android:layout_height="15dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/download"
        android:scaleType="fitXY"
        android:layout_marginLeft="18dp"
        />
    <TextView
        android:id="@+id/recycle_white_txt"
        android:layout_below="@+id/recycle_white_sign"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/login_small_title"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="18dp"
        android:text="aaDDDDa"
        android:textColor="@color/whiteItem"
        android:gravity="center"/>
</com.example.simple.simplethink.totle.fragment.whiteNoisePage.MyLayout>
