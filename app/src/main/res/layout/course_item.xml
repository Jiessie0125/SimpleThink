<?xml version="1.0" encoding="utf-8"?>
<com.example.simple.simplethink.totle.RoundAngleFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android_custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    app:radius="10dp">
    <ImageView
        android:id="@+id/recycle_course_img"
        android:layout_width="155dp"
        android:layout_height="115dp"
        android:scaleType="fitXY" />
    <TextView
        android:id="@+id/recycle_course_vip"
        android:layout_width="37dp"
        android:layout_height="18dp"
        android:background="@color/fisrtUse"
        android:gravity="center"
        android:textSize="@dimen/logon_revive_word"
        android:textColor="@color/wordWhite"/>
    <TextView
        android:id="@+id/recycle_course_tv"
        android:layout_width="155dp"
        android:layout_height="115dp"
        android:textSize="@dimen/course_txt"
        android:textColor="@color/wordWhite"
        android:gravity="center"/>
</com.example.simple.simplethink.totle.RoundAngleFrameLayout>
